{
  "name": "@yama-dev/js-parse-module",
  "version": "0.2.0",
  "description": "javascript parse utility library.",
  "keywords": [
    "parse",
    "plugin"
  ],
  "author": "yama-dev",
  "license": "MIT",
  "main": "./dist/js-parse-module.js",
  "scripts": {
    "start": "npm install && npm run develop",
    "develop": "npm-run-all -p webpack:develop server",
    "production": "npm-run-all -p webpack:build server",
    "server": "browser-sync start --server ./ --directory ./sample --files **/*.css **/*.js **/*.html",
    "webpack:develop": "webpack --progress --colors --watch",
    "webpack:build": "webpack --mode=production --progress --colors --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yama-dev/js-parse-module"
  },
  "bugs": {
    "url": "https://github.com/yama-dev/js-parse-module/issues",
    "email": "tatsuya.yamamoto69@gmail.com"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.7",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6"
  }
}
